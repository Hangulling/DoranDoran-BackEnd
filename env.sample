# DoranDoran MSA 환경 변수 설정
# 이 파일을 .env로 복사하여 사용하세요

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=dorandoran
DB_USER=doran
DB_PASSWORD=doran

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379

# AWS S3 설정 (로컬 개발용)
AWS_REGION=ap-northeast-2
S3_BUCKET=dd-bucket
S3_ENDPOINT=http://localhost:4566
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test

# JWT 설정
JWT_SECRET=your-secret-key-here
JWT_EXPIRATION=86400000

# 로그 레벨
LOG_LEVEL=DEBUG

# 서비스 포트 설정
AUTH_SERVICE_PORT=8081
USER_SERVICE_PORT=8082
CHAT_SERVICE_PORT=8083
BATCH_SERVICE_PORT=8085
GATEWAY_PORT=8080
